<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core">
	<VBox class="sapUiSmallMargin">
		<f:Form id="FormDisplayITP" editable="false">
			<f:title>
				<core:Title text="Address"/>
			</f:title>
			<f:layout>
				<f:ResponsiveGridLayout labelSpanXL="4" labelSpanL="4" labelSpanM="4" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="1" emptySpanL="1"
					emptySpanM="1" emptySpanS="0" columnsXL="1" columnsL="1" columnsM="1" singleContainerFullSize="false"/>
			</f:layout>
			<f:formContainers>
				<f:FormContainer>
					<f:formElements>
						<f:FormElement label="Nume">
							<f:fields>
								<Text text="{localModel>/Nume}" id="NumeT"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="Prenume">
							<f:fields>
								<Text text="{localModel>/Prenume}" id="PrenumeT"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="Marca">
							<f:fields>
								<Text text="{localModel>/Marca}" id="marcaT"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="Numar de inmatriculare">
							<f:fields>
								<Text text="{localModel>/Numar}" id="numarT"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="Valabilitate ITP">
							<f:fields>
								<Text text="{localModel>/DataItp}" id="dItp"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="Pret ITP">
							<f:fields>
								<Text text="{localModel>/PretItp}" id="pItp"/>
							</f:fields>
						</f:FormElement>
					</f:formElements>
				</f:FormContainer>
			</f:formContainers>
		</f:Form>
	</VBox>
</core:FragmentDefinition>