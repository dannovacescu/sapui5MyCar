						<!--	<TileContent>-->
						<!--		<ImageContent src='sap-icon://add'/>-->
						<!--	</TileContent>-->
						<!--</GenericTile>-->
						<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Carte De Identitate" subheader="Buletin" press="onID">
							<TileContent>
								<ImageContent src='./images/id.png'/>
							</TileContent>
						</GenericTile>
						<!--<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Adaugare Pasaport" press="onAddPass">-->
						<!--	<TileContent>-->
						<!--		<ImageContent src='sap-icon://add' press="onAddPass1"/>-->
						<!--	</TileContent>-->
						<!--</GenericTile>-->
						<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Pasaport" subheader="Pasaport" press="onPass">
							<TileContent>
								<ImageContent src='./images/passport.png'/>
							</TileContent>
						</GenericTile>
						<!--<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Adaugare Card De Credit" press="onAddCard">-->
						<!--	<TileContent>-->
						<!--		<ImageContent src='sap-icon://add'/>-->
						<!--	</TileContent>-->
						<!--</GenericTile>-->
						<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Card De Credit" subheader="Card" press="onCard">
