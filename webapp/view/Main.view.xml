<mvc:View controllerName="MyCars.MyCars.controller.Main" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page1" title="{i18n>title}" titleAlignment="Center">
					<content>
						<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Adaugare Masina Noua" press="onAdd">
							<TileContent>
								<ImageContent src='sap-icon://add'/>
							</TileContent>
						</GenericTile>
						<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Garaj Masini" subheader="Masina mea" press="onList">
							<TileContent>
								<ImageContent src='./images/icon.png'/>
							</TileContent>
						</GenericTile>
						<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Verificare Asigurare Auto" subheader="RCA" press="onRCA">
