							<TileContent>
								<ImageContent src='./images/accident.png'/>
							</TileContent>
						</GenericTile>
						<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Verificare Rovinieta" subheader="Rovinieta"
							press="onRovinieta">
							<TileContent>
								<ImageContent src='./images/road.png'/>
							</TileContent>
						</GenericTile>
						<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Inspectia Tehnica Periodica" subheader="ITP" press="onTest">
							<TileContent>
								<ImageContent src='./images/itp.png'/>
							</TileContent>
						</GenericTile>
						<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Rapoarte" press="onRaport">
							<TileContent >
								<NumericContent value="2" valueColor="Critical" withMargin="false"/>
							</TileContent>
						</GenericTile>
