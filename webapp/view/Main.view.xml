								<ImageContent src='./images/card.png'/>
							</TileContent>
						</GenericTile>
						<!--<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Adaugare Eveniment" press="onAddEvent">-->
						<!--	<TileContent>-->
						<!--		<ImageContent src='sap-icon://add'/>-->
						<!--	</TileContent>-->
						<!--</GenericTile>-->
